<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.AppCompat.NoActionBar" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="change_one">
        <item name="android:button">@drawable/radio_one</item>
    </style>

    <style name="change_two">
        <item name="android:button">@drawable/radio_two</item>
    </style>

    <style name="change_three">
        <item name="android:button">@drawable/radio_three</item>
    </style>

    <style name="change_four">
        <item name="android:button">@drawable/radio_four</item>
    </style>

</resources>
